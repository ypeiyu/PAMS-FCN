    top: "ave_right_body_loc_p3"
    name: "ave_right_body_loc_p3"
    type: "Pooling"
    pooling_param {
        pool: AVE
        kernel_h: 1
        kernel_w: 28
        stride: 1
    }
}

#==== left_body ====
layer {
    bottom: "left_body_half_loc_p3"
    top: "ave_left_body_half_loc_p3"
    name: "ave_left_body_half_loc_p3"
    type: "Pooling"
    pooling_param {
        pool: AVE
        kernel_h: 1
        kernel_w: 40
        stride: 1
    }
}





layer {
  name: "part_concat_loc_p3"
  bottom: "ave_head_loc_p3"
  bottom: "ave_top_thirty_loc_p3"
  bottom: "ave_left_body_loc_p3"
  bottom: "ave_right_body_loc_p3"
  bottom: "ave_left_body_half_loc_p3"


  top: "part_concat_loc_p3"


  type: "Concat"
  concat_param {
    axis: 3
  }
}


layer {
    bottom: "part_concat_loc_p3"
    top: "local_bbox_pred_p3"
    name: "max_bbox_pred_rois_p3"
    type: "Pooling"
    pooling_param {
        pool: MAX
        kernel_h: 1
        kernel_w: 5
        stride: 1
    }
}
###########
layer {
    bottom: "global_bbox_pred_p3"
    bottom: "local_bbox_pred_p3"
    top: "bbox_pred_pre_p3"
    name: "bbox_pred_p3"
    type: "Eltwise"
}
##################### end couple ###################################


